        <record id="cabys_catalog_import_wizard_form_view" model="ir.ui.view">
            <field name="name">cabys.catalog.import.wizard.form</field>
            <field name="model">cabys.catalog.import.wizard</field>
            <field name="arch" type="xml">
                <form>

					<field name="notes"/>

					<field name="cabys_excel_file" colspan="2" string="Cargar catálogo en archivo de Excel" />			

					<!-- <button string="Descargar catálogo del BCCR" class="btn btn-default" type="object" name="download_catalog" attrs="{'invisible':[('cabys_excel_file','!=',False)]}"/> -->
					<!-- <field name="file_url"/> -->

                    <footer>
						<field name="button_enable" invisible="1"/>
                        <button string="Importar Catálogo" name="upload_catalog" type="object" class="oe_highlight" attrs="{'invisible':[('button_enable','=',False)]}"/>
                        <button string="Cancelar" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


	</data>

	
</odoo>